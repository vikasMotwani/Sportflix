<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("head")  %>
</head>
<body>
    <%- include("navbar")  %>
    <% if (res.length > 0) { %>
        <% res.forEach(items => { %>
            <div>
                <div><%- include("card", { item: items,status:""}) %></div>
            </div>
        <% }) %>
    <% } else { %>
        <div>No results found</div>
    <% } %>
    <%-include("footer")  %>
</body>
</html>
